{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "lcone",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "windows": [
      {
        "label": "main",
        "url": "/",
        "fileDropEnabled": true,
        "center": true,
        "x": 0,
        "y": 0,
        "width": 1280,
        "height": 700,
        "minWidth": 800,
        "minHeight": 600,
        "maxWidth": 1366,
        "maxHeight": 768,
        "resizable": true,
        "title": "lcone",
        "fullscreen": false,
        "focus": true,
        "transparent": false,
        "maximizable": true,
        "visible": true,
        "decorations": false,
        "alwaysOnTop": false,
        "contentProtected": false,
        "skipTaskbar": false,
        "theme": "Light",
        "titleBarStyle": "Visible",
        "hiddenTitle": false,
        "acceptFirstMouse": false
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "wen.suwenmao.tool",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico",
        "icons/icon.png",
        "icons/StoreLogo.png"
      ],
      "windows": {
        "allowDowngrades": true,
        "certificateThumbprint": null,
        "digestAlgorithm": null,
        "nsis": {
          "license": "xxxx",
          "installerIcon": "icons/32x32.png",
          "installMode": "both",
          "languages": [
            "zh-CN",
            "zh-TW",
            "en-US",
            "jp-JP"
          ],
          "displayLanguageSelector": false
        },
        "timestampUrl": null,
        "tsp": false,
        "webviewFixedRuntimePath": null,
        "webviewInstallMode": {
          "silent": true,
          "type": "offlineInstaller"
        },
        "wix": {
          "language": [
            "zh-CN",
            "zh-TW",
            "en-US",
            "jp-JP"
          ]
        }
      }
    }
  }
}