{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "lcone",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "label": "main",
        "title": "lcone",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "wen.suwenmao.tool",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "windows": {
        "allowDowngrades": true,
        "certificateThumbprint": null,
        "digestAlgorithm": null,
        "nsis": {
          "license": "xxxx",
          "installerIcon": "icons/32x32.png",
          "installMode": "both",
          "languages": [
            "zh-CN",
            "zh-TW",
            "en-US",
            "jp-JP"
          ],
          "displayLanguageSelector": false
        },
        "timestampUrl": null,
        "tsp": false,
        "webviewFixedRuntimePath": null,
        "webviewInstallMode": {
          "silent": true,
          "type": "offlineInstaller"
        },
        "wix": {
          "language": [
            "zh-CN",
            "zh-TW",
            "en-US",
            "jp-JP"
          ]
        }
      }
    }
  }
}
